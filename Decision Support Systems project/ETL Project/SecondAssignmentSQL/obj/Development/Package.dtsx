                  expressionType="Notify"
                  name="SqlCommand"
                  UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor">select addresses.BusinessEntityID, 
	   isnull(addresses.city, 'NoCity') as city, 
	   isnull(addresses.StateProvinceID, 0) as StateProvinceID, 
	   isnull(addresses.CountryRegionCode, 'ZZ') as CountryRegionCode from 
		(select addP.BusinessEntityID, addP.City, addP.StateProvinceID, addP.CountryRegionCode from (
		select 
		ROW_NUMBER() OVER(PARTITION BY p.BusinessEntityID ORDER BY p.BusinessEntityID) AS RN, 
		p.BusinessEntityID, 
		a.City, 
		a.StateProvinceID,
		st.CountryRegionCode
		from Person.Person as p
		left join Person.BusinessEntityAddress as ad on p.BusinessEntityID = ad.BusinessEntityID
		left join Person.Address as a on a.AddressID = ad.AddressID
		left join Person.StateProvince as st on a.StateProvinceID = st.StateProvinceID) as addP
		where addP.RN = 1
		union  all
		select stor.BusinessEntityID, stor.City, stor.StateProvinceID, stor.CountryRegionCode from 
		(select ROW_NUMBER() OVER(PARTITION BY a.BusinessEntityID ORDER BY a.BusinessEntityID) AS RN,
		a.BusinessEntityID, a.City, b.StateProvinceID,  b.CountryRegionCode from sales.vStoreWithAddresses as a
		join person.vStateProvinceCountryRegion as b on a.StateProvinceName = b.StateProvinceName and a.CountryRegionName = b.CountryRegionName) as stor
